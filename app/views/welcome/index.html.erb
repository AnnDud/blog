<h1>My app</h1>


<%= form_tag articles_path, method: :get do %>
  <div class="input-group">
    <%= text_field_tag :q, "", class: "form-control" %>
    <span class="input-group-btn">
      <%= submit_tag "Search", class: "btn btn-default" %>
    </span>
  </div>
<% end %>

<%= link_to 'My Blog', articles_path %>